{
    "options": {
        "sct_output": ".bin/UK.sct",
        "ese_output": ".bin/UK.ese",
        "rwy_output": ".bin/UK.rwy",
        "replace": [
            {
                "token": "{YEAR}",
                "type": "date",
                "format": "yyyy"
            },
            {
                "token": "{BUILD}",
                "type": "date",
                "format": "yyyy-MM-dd HH:mm:ss"
            },
            {
                "token": "{VERSION}",
                "type": "version"
            }
        ]
    },
    "includes": {
        "airports": {
            "Airports": {
                "airspace": {
                    "type": "files",
                    "files": [
                        "Airspace.txt"
                    ],
                    "ignore_missing": true
                },
                "basic": {
                    "type": "files",
                    "files": [
                        "Basic.txt"
                    ]
                },
                "centrelines": {
                    "type": "files",
                    "files": [
                        "Centreline.txt"
                    ],
                    "ignore_missing": true
                },
                "fixes": {
                    "type": "files",
                    "files": [
                        "Fixes.txt"
                    ],
                    "ignore_missing": true
                },
                "freetext": {
                    "type": "files",
                    "files": [
                        "Freetext.txt",
                        "SMAA_Bases.txt",
                        "Airspace_Bases.txt"
                    ],
                    "ignore_missing": true
                },
                "geo": {
                    "type": "files",
                    "files": [
                        "Range_Rings.txt"
                    ],
                    "ignore_missing": true
                },
                "ground_network": {
                    "type": "files",
                    "files": [
                        "Ground_Network.txt"
                    ],
                    "ignore_missing": true
                },
                "ownership": [
                  {
                    "type": "files",
                    "files": [
                        "Ownership.txt"
                    ],
                    "ignore_missing": true,
                    "exclude_directory": ["EGGD", "EGTE", "EGFF"]
                  }, 
                  {
                    "type": "files",
                    "files": [
                        "Ownership.txt"
                    ],
                    "ignore_missing": true,
                    "exclude_directory": ["EGFF", "EGTE"]
                  },
                  {
                    "type": "files",
                    "files": [
                        "Ownership.txt"
                    ],
                    "ignore_missing": true,
                    "exclude_directory": ["EGFF"]
                  },
                  {
                    "type": "files",
                    "files": [
                        "Ownership.txt"
                    ],
                    "ignore_missing": true
                  }
                ],
                "positions": {
                    "type": "files",
                    "files": [
                        "Positions.txt"
                    ],
                    "ignore_missing": true
                },
                "positions_mentor": {
                    "type": "files",
                    "files": [
                        "Positions_Mentor.txt"
                    ],
                    "ignore_missing": true
                },
                "runways": {
                    "type": "files",
                    "files": [
                        "Runway.txt"
                    ],
                    "ignore_missing": true
                },
                "sectors": {
                    "type": "files",
                    "files": [
                        "Sector.txt"
                    ],
                    "ignore_missing": true
                },
                "sids": {
                    "type": "files",
                    "files": [
                        "Sids.txt"
                    ],
                    "ignore_missing": true
                },
                "sid_airspace": {
                    "type": "files",
                    "files": [
                        "SMAA.txt"
                    ],
                    "ignore_missing": true
                },
                "stars": {
                    "type": "files",
                    "files": [
                        "Stars.txt"
                    ],
                    "ignore_missing": true
                },
                "star_airspace": {
                    "type": "files",
                    "files": [
                        "RMA.txt"
                    ],
                    "ignore_missing": true
                },
                "smr": {
                    "geo": {
                        "type": "files",
                        "files": [
                            "SMR/Geo.txt"
                        ],
                        "ignore_missing": true
                    },
                    "regions": {
                        "type": "files",
                        "files": [
                            "SMR/Regions.txt"
                        ],
                        "ignore_missing": true
                    },
                    "labels": {
                        "type": "files",
                        "files": [
                            "SMR/Labels.txt"
                        ],
                        "ignore_missing": true
                    }
                },
                "ground_map": {
                    "geo": {
                        "type": "files",
                        "files": [
                            "Ground Map/Geo.txt"
                        ],
                        "ignore_missing": true,
                        "except_where_exists": "SMR/Geo.txt"
                    },
                    "regions": {
                        "type": "files",
                        "files": [
                            "Ground Map/Regions.txt"
                        ],
                        "ignore_missing": true,
                        "except_where_exists": "SMR/Regions.txt"
                    },
                    "labels": {
                        "type": "files",
                        "files": [
                            "Ground Map/Labels.txt"
                        ],
                        "ignore_missing": true,
                        "except_where_exists": "SMR/Labels.txt"
                    }
                },
                "vrps": {
                    "type": "files",
                    "files": [
                        "VRPs.txt"
                    ],
                    "ignore_missing": true
                }
            }
        },
        "enroute": {
            "sector_lines": {
                "type": "folder",
                "folder": "Sectors/"
            },
            "ownership": [
                {
                    "type": "folder",
                    "folder": "Ownership/TC - LTC"
                },
                {
                    "type": "folder",
                    "folder": "Ownership/TC - MPC"
                },
                {
                    "type": "folder",
                    "folder": "Ownership/TC - STC"
                },
                {
                    "type": "folder",
                    "folder": "Ownership/AC - Central"
                },
                {
                    "type": "folder",
                    "folder": "Ownership/AC - North"
                },
                {
                    "type": "folder",
                    "folder": "Ownership/AC - South"
                },
                {
                    "type": "folder",
                    "folder": "Ownership/AC - West"
                },
                {
                    "type": "folder",
                    "folder": "Ownership/ScAC - East"
                },
                {
                    "type": "folder",
                    "folder": "Ownership/ScAC - West"
                },
                {
                    "type": "folder",
                    "folder": "Ownership/Swanwick Mil"
                },
                {
                    "type": "folder",
                    "folder": "Ownership/Alternate",
                    "recursive": true
                },
                {
                    "type": "folder",
                    "folder": "Ownership/Non-UK"
                }
            ],
            "positions": {
                "type": "folder",
                "folder": "Misc",
                "recursive": true,
                "pattern": "Positions_"
            }
        },
        "misc": {
            "agreements": [
                {
                    "type": "folder",
                    "folder": "Agreements/Airport",
                    "recursive": true
                },
                {
                    "type": "folder",
                    "folder": "Agreements/Internal",
                    "recursive": true
                },
                {
                    "type": "folder",
                    "folder": "Agreements/External",
                    "recursive": true,
                    "exclude": [
                        "1_RR-Q.txt",
                        "4_SNE.txt",
                        "6_RR-V.txt",
                        "7_SNE.txt",
                        "12_AA.txt"
                    ]
                },
                {
                    "type": "files",
                    "files": [
                        "Agreements/External/1_RR-Q.txt",
                        "Agreements/External/4_SNE.txt",
                        "Agreements/External/6_RR-V.txt",
                        "Agreements/External/7_SNE.txt",
                        "Agreements/External/12_AA.txt"
                    ]
                }
            ],
            "freetext": {
                "type": "folder",
                "folder": "Misc",
                "recursive": true,
                "pattern": "Freetext_"
            },
            "colours": {
                "type": "files",
                "files": [
                    "Colours.txt"
                ]
            },
            "info": {
                "type": "files",
                "files": [
                    "Info.txt"
                ]
            },
            "file_headers": {
                "type": "files",
                "files": [
                    "Comments.txt"
                ]
            },
            "pre_positions": {
                "type": "files",
                "files": [
                    "Misc/Pre-Positions.txt"
                ]
            },
            "fixes": [
                {
                    "type": "folder",
                    "folder": "Navaids",
                    "recursive": true,
                    "pattern": "FIXES_"
                },
            ],
            "ndbs": {
                "type": "folder",
                "folder": "Navaids",
                "recursive": true,
                "pattern": "NDB_"
            },
            "vors": {
                "type": "folder",
                "folder": "Navaids",
                "recursive": true,
                "pattern": "VOR_"
            },
            "danger_areas": {
                "type": "folder",
                "folder": "ARTCC/Danger Areas"
            },
            "radars": {
                "type": "files",
                "files": [
                    "Misc/Radar Sites.txt"
                ]
            },
            "radar_holes": {
                "type": "files",
                "files": [
                    "Misc/Radar Holes.txt"
                ]
            },
            "artcc_low": {
                "type": "folder",
                "folder": "ARTCC/Low"
            },
            "artcc_high": {
                "type": "folder",
                "folder": "ARTCC/High"
            },
            "lower_airways": [
                {
                    "type": "folder",
                    "folder": "Airways/RNAV/Lower"
                },
                {
                    "type": "folder",
                    "folder": "Airways/Conventional - Non-UK/Lower"
                },
                {
                    "type": "folder",
                    "folder": "Airways/Helicopter"
                }
            ],
            "upper_airways": [
                {
                    "type": "folder",
                    "folder": "Airways/Other"
                },
                {
                    "type": "folder",
                    "folder": "Airways/RNAV/Upper"
                }
            ],
            "sid_airspace": [
                {
                    "type": "folder",
                    "folder": "Sectors/Static",
                    "recursive": true
                },
                {
                "type": "folder",
                "folder": "Misc",
                "recursive": true,
                "pattern": "SID_"
                },
            ],
            "star_airspace": [
                {
                    "type": "folder",
                    "folder": "Sectors/Static",
                    "recursive": true
                },
                {
                    "type": "folder",
                    "folder": "Misc",
                    "recursive": true,
                    "pattern": "STAR_"
                }
            ],
            "geo": {
                "type": "folder",
                "folder": "Misc",
                "recursive": true,
                "pattern": "Geo_",
                "exclude": [
                    "Geo_Coastline High Detail.txt"
                ]
            },
            "regions": {
                "type": "folder",
                "folder": "Misc",
                "recursive": true,
                "pattern": "Regions_"
            },
            "active_runways": {
                "type": "files",
                "files": [
                    "UK.rwy"
                ]
            }
        }
    }
}
